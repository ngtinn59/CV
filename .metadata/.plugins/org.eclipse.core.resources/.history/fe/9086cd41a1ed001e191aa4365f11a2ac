package cit.ctu;

public class KhachHang {
	private int mso;
	private String hoten;
	private String diachi;
    private String gtinh;
    
	public int getMso() {
		return mso;
	}
	public void setMso(int mso) {
		this.mso = mso;
	}
	public String getHoten() {
		return hoten;
	}
	public void setHoten(String hoten) {
		this.hoten = hoten;
	}
	public String getDiachi() {
		return diachi;
	}
	public void setDiachi(String diachi) {
		this.diachi = diachi;
	}
	public String getGtinh() {
		return gtinh;
	}
	public void setGtinh(Boolean gtinh) {
		this.gtinh = gtinh;
	}
	public KhachHang(int mso, String hoten, String diachi, Boolean gtinh) {
		super();
		this.mso = mso;
		this.hoten = hoten;
		this.diachi = diachi;
		this.gtinh = gtinh;
	}
	public KhachHang() {
		super();
		// TODO Auto-generated constructor stub
	}
	
    
	
    
	
	
}
